{
  "name": "nextdash-b",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "worker": "tsx worker/src/index.ts",
    "worker:dev": "tsx watch worker/src/index.ts",
    "migrate": "tsx database/migrate.ts",
    "seed": "tsx database/seed.ts",
    "setup-db": "node scripts/setup-database.js",
    "setup-postgres": "node scripts/setup-postgres.js",
    "test-postgres": "node scripts/setup-postgres.js test",
    "deploy": "./scripts/deploy-vercel.sh",
    "deploy:preview": "vercel",
    "test-db": "node scripts/test-connection.js",
    "diagnose-db": "node scripts/diagnose-connection.js",
    "setup-simple": "node scripts/setup-simple.js",
    "setup-env": "node scripts/setup-env-users.js",
    "docker:build": "docker build -t nextdash-b .",
    "docker:run": "docker run -p 3000:3000 --env-file .env nextdash-b",
    "docker:up": "./scripts/docker-setup.sh",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:hostinger": "./scripts/docker-hostinger.sh",
    "docker:hostinger-build": "docker build -f Dockerfile.hostinger -t nextdash-hostinger .",
    "docker:hostinger-up": "docker-compose -f docker-compose.hostinger.yml up -d",
    "docker:hostinger-down": "docker-compose -f docker-compose.hostinger.yml down"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@ant-design/plots": "^2.2.5",
    "@types/pg": "^8.15.4",
    "antd": "^5.20.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "fingerprintjs2": "^2.1.4",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "pg": "^8.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "sharp": "^0.33.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.14",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
